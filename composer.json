{
  "name": "MitkoTochev/MyApp",
  "description": "Programing.",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "fideloper/proxy": "~3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "6.5.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/App/Tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
